<main>
    <h1>Articles</h1>
    <% if (typeof articles === "object" && articles) { %> <% for(article of articles) { %>
    <div>
        <a href="<%= article.link %>" target="_blank" class="card">
            <h2><%= article.title %></h2>
            <p><%= article.description %></p>
            <a href="/<%= article.id %>" class="article-link">&#128172; Comment</a>
        </a>
    </div>
    <% } %> <% } else { %>
    <h2>No articles found</h2>
    <% } %>
</main>
